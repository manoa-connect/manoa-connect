{
  "name": "manoa-connect-app",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-https": "next dev --experimental-https",
    "build": "prisma generate && next build",
    "playwright-development": "npx playwright test",
    "playwright-ci": "next dev && npx playwright test",
    "start": "meteor --no-release-check --exclude-archs web.browser.legacy,web.cordova --settings ../config/settings.development.json",
    "lint": "eslint --quiet --ext .jsx --ext .js ./imports && eslint --quiet --ext .js ./tests",
    "lint-fix": "eslint --quiet --fix --ext .jsx --ext .js ./imports && eslint --quiet --ext .js ./tests",
    "testcafe": "testcafe chrome tests/*.testcafe.js",
    "testcafe-single": "testcafe chrome tests/*.testcafe.js -t \"Test that signin and signout work\"",
    "testcafe-ci": "testcafe chrome:headless tests/*.testcafe.js -q --app \"meteor npm run start\"",
    "test-all": "meteor reset && meteor npm run lint && meteor npm run testcafe-ci",
    "unimported": "npx unimported"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@ngneat/falso": "^7.2.0",
    "@playwright/test": "^1.52.0",
    "@prisma/client": "^6.6.0",
    "@supabase/ssr": "^0.6.1",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "browser-image-compression": "^2.0.2",
    "lint": "^1.1.2",
    "multiselect-react-dropdown": "^2.0.25",
    "next": "^14.2.24",
    "next-auth": "^4.24.7",
    "playwright": "^1.52.0",
    "react": "^18",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "supabase": "^2.22.6",
    "sweetalert": "^2.1.2",
    "ts-node": "10.9.2",
    "uuid": "^11.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.5.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.10",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.1",
    "prettier-eslint": "^16.3.0",
    "prisma": "^6.6.0",
    "typescript": "^5.4.5"
  }
}
